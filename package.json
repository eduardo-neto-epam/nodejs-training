  "description": "Homework tasks for Node.js training.",
  "main": "./dist/index.js",
  "scripts": {
    "task_1:start": "nodemon --exec babel-node ./misc/task_1/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "tsc": "tsc",
    "start": "nodemon --exec babel-node src/index.ts --extensions \".ts\"",
    "build": "npm run tsc && babel src -d dist --extensions \".ts\"",
    "migrations:run": "npx sequelize-cli db:migrate",
    "migrations:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
